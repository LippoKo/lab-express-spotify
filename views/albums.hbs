<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Albums</title>
	</head>
	<body>
		<div class="album">
			{{#each albums}}
				<h3>{{this.name}}</h3>
				<img src="{{this.images.0.url}}" alt="album image" />
				<button><a href="/tracks/{{this.id}}">Tracks</a></button>
			{{/each}}
		</div>
	</body>
</html>